<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	 #### AUCTION AGENTS ###
	<agent name="auction-Random">
		<set class-path="bin/"/>
		<set class-name="Random.randomAgent"/>
	</agent>
	<agent name="auction-SemiRandom">
		<set class-path="bin/"/>
		<set class-name="Random.semiRandomAgent"/>
	</agent>
	<agent name="auction-Baseline">
		<set class-path="bin/"/>
		<set class-name="AuctionSmart.Auction"/>
		<set timePerGlobalScenario="5000"/>
		<set horizon="4"/>
		<set adaptationRatio="0.1"/>
	</agent>
	<agent name="auction-AR0.2">
		<set class-path="bin/"/>
		<set class-name="AuctionSmart.Auction"/>
		<set timePerGlobalScenario="5000"/>
		<set horizon="4"/>
		<set adaptationRatio="0.2"/>
	</agent>

</agents>
